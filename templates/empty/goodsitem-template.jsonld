{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://github.com/jgmikael/trade-automation/contexts/goodsitem-context.jsonld"
  ],
  "id": "<<CREDENTIAL_ID>>",
  "type": [
    "VerifiableCredential",
    "GoodsItemCredential"
  ],
  "issuer": {
    "id": "<<ISSUER_DID>>",
    "name": "<<ISSUER_NAME>>"
  },
  "issuanceDate": "<<ISO_8601_DATETIME>>",
