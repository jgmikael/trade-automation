{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://github.com/jgmikael/trade-automation/contexts/location-context.jsonld"
  ],
  "id": "<<CREDENTIAL_ID>>",
  "type": [
    "VerifiableCredential",
    "LocationCredential"
  ],
  "issuer": {
    "id": "<<ISSUER_DID>>",
    "name": "<<ISSUER_NAME>>"
  },
  "issuanceDate": "<<ISO_8601_DATETIME>>",
  "credentialSubject": {
    "id": "<<SUBJECT_ID>>",
    "type": "Location",
    "locationCountry": [
      {
        "type": "Country",
        "<<PROPERTY>>": "<<VALUE for Country>>"
      }
    ],
    "locationName": "<<Location Name>>",
    "unlocode": "<<UN Locode>>"
  },
  "proof": {
    "type": "<<SIGNATURE_TYPE>>",
    "created": "<<ISO_8601_DATETIME>>",
    "verificationMethod": "<<ISSUER_DID#KEY_ID>>",
    "proofPurpose": "assertionMethod",
    "proofValue": "<<SIGNATURE_VALUE>>"
  }
}