{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    "https://github.com/jgmikael/trade-automation/contexts/tradeitem-context.jsonld"
  ],
  "id": "<<CREDENTIAL_ID>>",
  "type": [
    "VerifiableCredential",
    "TradeItemCredential"
  ],
  "issuer": {
    "id": "<<ISSUER_DID>>",
    "name": "<<ISSUER_NAME>>"
  },
  "issuanceDate": "<<ISO_8601_DATETIME>>",
